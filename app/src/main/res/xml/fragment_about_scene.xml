<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_editor_absoluteY="643dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="201dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView3" />
        <Constraint
            motion:layout_editor_absoluteY="154dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="176dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView" />
        <Constraint
            motion:layout_editor_absoluteY="422dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="188dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView2" />
        <Constraint
            android:layout_height="@dimen/min_height"
            android:layout_width="wrap_content"
            android:id="@+id/textViewTnC"
            motion:layout_constraintTop_toBottomOf="@+id/textViewJoshua"
            motion:layout_constraintEnd_toEndOf="@+id/textViewJoshua"
            motion:layout_constraintStart_toStartOf="@+id/textViewJoshua"
            android:layout_marginTop="30dp" />
        <Constraint
            android:layout_height="@dimen/min_height"
            android:layout_width="wrap_content"
            android:id="@+id/textViewJoshua"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintTop_toBottomOf="@+id/imageView2"
            android:layout_marginTop="40dp" />
        <Constraint
            android:layout_height="@dimen/min_height"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textViewVersion"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.534"
            motion:layout_constraintTop_toBottomOf="@+id/textViewTnC"
            android:layout_marginTop="40dp" />
        <Constraint
            android:layout_height="@dimen/min_height"
            android:layout_width="218dp"
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.497"
            android:layout_marginTop="40dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>